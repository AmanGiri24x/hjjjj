{
  "name": "dhanaillytics-backend",
  "version": "1.0.0",
  "description": "DhanAillytics Backend API - AI-powered financial analytics platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:no-check": "nodemon --exec \"ts-node --transpile-only src/server.ts\"",
    "demo": "nodemon --exec \"ts-node --transpile-only src/demo-server.ts\"",
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "seed": "ts-node src/utils/seed.ts",
    "seed:drop": "ts-node src/utils/seed.ts --drop",
    "seed:users": "ts-node src/utils/seed.ts --users-only",
    "seed:market": "ts-node src/utils/seed.ts --market-data-only",
    "seed:portfolios": "ts-node src/utils/seed.ts --portfolios-only",
    "migrate": "ts-node src/utils/migrations.ts migrate",
    "migrate:status": "ts-node src/utils/migrations.ts status",
    "migrate:rollback": "ts-node src/utils/migrations.ts rollback",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "npm run seed:drop && npm run migrate && npm run seed",
    "health:check": "ts-node src/utils/healthCheck.ts report",
    "health:integrity": "ts-node src/utils/healthCheck.ts integrity",
    "test:api": "ts-node src/utils/testFinancialApi.ts"
  },
  "keywords": [
    "fintech",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "typescript",
    "ai",
    "analytics"
  ],
  "author": "DhanAillytics Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.15",
    "qrcode": "^1.5.4",
    "redis": "^5.8.0",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
